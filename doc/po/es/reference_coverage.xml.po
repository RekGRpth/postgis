#. Tag: refname
#: reference_coverage.xml:132
#, no-c-format
msgid "ST_CoverageUnion"
msgstr ""

#. Tag: refpurpose
#: reference_coverage.xml:134
#, no-c-format
msgid "Unions a set of polygons forming a coverage by removing shared edges."
msgstr ""

#. Tag: funcprototype
#: reference_coverage.xml:139
#, no-c-format
msgid ""
"<funcdef>geometry <function>ST_CoverageUnion</function></funcdef> "
"<paramdef><type>geometry set</type> <parameter>geom</parameter></paramdef>"
msgstr ""

#. Tag: para
#: reference_coverage.xml:150
#, no-c-format
msgid ""
"An aggregate function which performs a high-speed union of a set of polygons "
"forming a polygonal coverage. This function only works with inputs that are "
"coverages: no overlaps, and all shared edges identical down to the vertex "
"level. See <xref linkend=\"ST_CoverageInvalidLocations\"/> for a way to test "
"that a set of polygons are a valid polygonal coverage."
msgstr ""

#. Tag: para
#: reference_coverage.xml:153
#, no-c-format
msgid ""
"If the input is not a valid coverage there may be unexpected artifacts in "
"the output (such as unmerged or overlapping polygons)."
msgstr ""

#. Tag: para
#: reference_coverage.xml:155
#, no-c-format
msgid "Availability: 3.4.0 - requires GEOS &gt;= 3.8.0"
msgstr ""

#. Tag: programlisting
#: reference_coverage.xml:161
#, no-c-format
msgid ""
"CREATE TABLE coverage (id integer, geom geometry);\n"
"\n"
"INSERT INTO coverage VALUES\n"
"  (1, 'POLYGON((0 0, 10 0, 10.1 5, 10 10, 0 10, 0 0))'),\n"
"  (2, 'POLYGON((10 0, 20 0, 20 10, 10 10, 10.1 5, 10 0))'),\n"
"  (3, 'POLYGON((20 0, 30 0, 30 10, 20 10, 20 0))');\n"
"\n"
"SELECT ST_AsText(ST_CoverageUnion(geom))\n"
"  FROM coverage;\n"
"\n"
"                        st_astext\n"
"----------------------------------------------------------\n"
" POLYGON((0 0,0 10,10 10,20 10,30 10,30 0,20 0,10 0,0 0))"
msgstr ""
