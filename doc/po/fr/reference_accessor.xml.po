msgid ""
"SELECT polyTable.field1, polyTable.field1,\n"
"          (ST_DumpRings(polyTable.geom)).geom As geom\n"
"FROM polyTable;"
msgstr ""
"SELECT polyTable.field1, polyTable.field1,\n"
"          (ST_DumpRings(polyTable.geom)).geom As geom\n"
"FROM polyTable ;"

