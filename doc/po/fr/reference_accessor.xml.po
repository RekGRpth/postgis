"        0 0 0\n"
"    ))\n"
"    )' ::geometry AS geom\n"
"        ) AS g\n"
") j ;\n"
"\n"
"  path   │        st_asewkt\n"
"  ---------------------------------\n"
" {1,1,1} │ LINESTRING(0 0 0,0 0 1)\n"
" {1,1,2} │ LINESTRING(0 0 1,0 1 0)\n"
" {1,1,3} │ LINESTRING(0 1 0,0 0 0)\n"
" {2,1,1} │ LINESTRING(0 0 0,0 1 0)\n"
" {2,1,2} │ LINESTRING(0 1 0,1 1 0)\n"
" {2,1,3} │ LINESTRING(1 1 0,0 0 0)\n"
"(6 lignes)"

