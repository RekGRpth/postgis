msgid ""
", <xref linkend=\"ST_MinimumBoundingCircle\"/>, <xref "
"linkend=\"ST_LongestLine\"/>"
msgstr ""
", <xref linkend=\"ST_MinimumBoundingCircle\"/>, <xref "
"linkend=\"ST_LongestLine\"/>"

#. Tag: refpurpose
