"                command.Parameters.Add(new NpgsqlParameter(\"input_srid\", "
"input_srid));\n"
"\n"
"\n"
"                            result = (byte[]) command.ExecuteScalar();\n"
"                conn.Close();\n"
"                        }\n"
"\n"
"                }\n"
"        catch (Exception ex)\n"
"        {\n"
"            result = null;\n"
"            context.Response.Write(ex.Message.Trim());\n"
"        }\n"
"                return result;\n"
"        }\n"
"}]]>"
