#, no-c-format
msgid "&list_dump_functions;"
msgstr "&list_dump_functions;"

#. Tag: title
#: postgis_special.xml:47
#, no-c-format
msgid "PostGIS Box Functions"
msgstr "Funzioni del box PostGIS"

#. Tag: para
#: postgis_special.xml:48
#, no-c-format
