#: reference_processing.xml:2162
#, no-c-format
msgid "Outer hull of a Polygon"
msgstr ""

#. Tag: para
#: reference_processing.xml:2174
#, no-c-format
msgid "Inner hull of a Polygon"
msgstr ""

#. Tag: para
#: reference_processing.xml:2186
#, no-c-format
